{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:8080"
      },
      "Https": {
        "Url": "https://+:8443",
        "Protocols": "Http1AndHttp2",
        "Certificate": {
          "Path": "path/to/your/certificate.pfx",
          "Password": "your_certificate_password"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "eff1cdfd1f24019b636bac1b4796040c6dc98d91bc0458e28dcbb2a6bd4c1732",
    "Issuer": "myIssuerForJwtToken"
  },
  "AllowedHosts": "*"
}
